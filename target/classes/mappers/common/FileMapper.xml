<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itbank.navercafe.comon.file.mapper.FileMapper">
	<resultMap type="com.itbank.navercafe.comon.file.dto.FileDTO" id="file">
        <id column="FILE_NUM" property="fileNum"/>
        <result column="FILE_DIRECTORY" property="fileDirectory"/>
        <result column="FILE_ORG_NAME" property="fileOrgName"/>
        <result column="FILE_STORED_NAME" property="fileStoredName"/>
        <result column="REGDATE" property="regdate"/>
        <result column="BOARD_NUM" property="boardNum"/>
        <result column="CAFE_TITLE_NUM" property="cafeTitleNum"/>
        <result column="REPLY_NUM" property="replyNum"/>
        <result column="CAFE_USER_IMAGE_NUM" property="cafeUserImageNum"/>
        <result column="CAFE_ICON_NUM" property="cafeIconNum"/>
        <result column="MEMO_REPLY_NUM" property="memoReplyNum"/>
    </resultMap>
	
	<insert id="insertAttachFile" >
		<![CDATA[INSERT INTO PROJECT_FILE(FILE_NUM, FILE_DIRECTORY, FILE_ORG_NAME, FILE_STORED_NAME, 
			BOARD_NUM, REPLY_NUM, CAFE_USER_IMAGE_NUM, CAFE_TITLE_NUM, REGDATE)
		VALUES(project_file_seq.nextval, #{fileDirectory}, #{fileOrgName}, #{fileStoredName}, 
			#{boardNum}, #{boardReplyNum}, #{profileNum}, #{titleNum}, sysdate)]]>
	</insert>
</mapper>

